{
  "name": "koa-oas-router",
  "version": "1.1.0",
  "description": "This module extends the koa-router with some features usable with oas.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "doc:api": "jsdoc2md --files ./src/**/*.ts --configure ./jsdoc2md.json > ./docs/API.md",
    "test": "ava",
    "start:test": "ts-node tmp/test.js",
    "prebuild": "npm run test",
    "build": "rimraf ./dist && tsc",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maHecht/koa-oas-router.git"
  },
  "keywords": [
    "koa",
    "oas",
    "koa-router"
  ],
  "author": "Matthias Hecht",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maHecht/koa-oas-router/issues"
  },
  "homepage": "https://github.com/maHecht/koa-oas-router#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/koa-router": "^7.0.42",
    "@types/logger": "0.0.0",
    "@types/node": "^12.7.1",
    "ava": "^2.2.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "koa-router": "^7.4.0",
    "logger": "0.0.1"
  }
}

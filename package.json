{
  "name": "koa-oas-router",
  "version": "1.2.2",
  "description": "This module extends the koa-router with some features usable with oas.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "doc:api": "jsdoc2md --files ./src/**/*.ts --configure ./jsdoc2md.json > ./docs/API.md",
    "test": "ava",
    "start:test": "ts-node tmp/test.js",
    "prebuild": "npm run test",
    "build": "rimraf ./dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matzehecht/koa-oas-router.git"
  },
  "keywords": [
    "koa",
    "oas",
    "koa-router"
  ],
  "author": "Matthias Hecht",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matzehecht/koa-oas-router/issues"
  },
  "homepage": "https://github.com/matzehecht/koa-oas-router#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/js-yaml": "^3.12.3",
    "@types/koa-router": "^7.4.0",
    "@types/logger": "0.0.0",
    "@types/node": "^12.12.36",
    "ava": "^2.4.0",
    "js-yaml": "^3.13.1",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^5.0.3",
    "rimraf": "^2.7.1",
    "ts-node": "^8.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "koa-router": "^8.0.8",
    "oas-validator": "^4.0.3",
    "simple-node-logger": "^18.12.24"
  }
}
